---
layout: null
permalink: /projects.json
---
[
{%- for p in site.projects -%}
  {
    "title": {{ p.title | jsonify }},
    "summary": {{ p.summary | jsonify }},
    "tech": {{ p.tech | jsonify }},
    "url": {{ p.url | relative_url | jsonify }},
    "repo_url": {{ p.repo_url | jsonify }},
    "live_url": {{ p.live_url | jsonify }},
    "order": {{ p.order | jsonify }}
  }{%- unless forloop.last -%},{%- endunless -%}
{%- endfor -%}
]
